<script>
// Filter functionality
document.addEventListener('DOMContentLoaded', function() {
    const filterBtns = document.querySelectorAll('.filter-btn');
    const breedCards = document.querySelectorAll('.breed-card');
    
    filterBtns.forEach(btn => {
        btn.addEventListener('click', () => {
            const filter = btn.getAttribute('data-filter');
            
            // Remove active class from siblings
            btn.parentElement.querySelectorAll('.filter-btn').forEach(b => b.classList.remove('active'));
            btn.classList.add('active');
            
            // Filter breed cards
            breedCards.forEach(card => {
                if (filter.includes('all') || filter.includes('any')) {
                    card.style.display = 'block';
                } else if (filter.includes('small') || filter.includes('medium') || filter.includes('large')) {
                    const size = card.getAttribute('data-size');
                    card.style.display = filter.includes(size) ? 'block' : 'none';
                } else if (filter.includes('activity')) {
                    const activity = card.getAttribute('data-activity');
                    const activityLevel = filter.replace('-activity', '');
                    card.style.display = activity === activityLevel ? 'block' : 'none';
                }
            });
        });
    });
});

function findBreeds() {
    const living = document.getElementById('living-situation').value;
    const experience = document.getElementById('experience').value;
    const exercise = document.getElementById('exercise-time').value;
    const resultDiv = document.getElementById('breed-quiz-result');
    
    if (living && experience && exercise) {
        let recommendations = [];
        
        if (living === 'apartment' && exercise === 'minimal') {
            recommendations = ['French Bulldog', 'Pug', 'Cavalier King Charles Spaniel'];
        } else if (exercise === 'lots' && experience === 'very-experienced') {
            recommendations = ['Border Collie', 'German Shepherd', 'Belgian Malinois'];
        } else if (experience === 'first-time') {
            recommendations = ['Golden Retriever', 'Labrador Retriever', 'Beagle'];
        } else {
            recommendations = ['Labrador Retriever', 'Golden Retriever', 'Beagle'];
        }
        
        resultDiv.innerHTML = `
            <div class="result-content">
                <strong>Recommended breeds:</strong><br>
                ${recommendations.map(breed => `<span class="breed-tag">${breed}</span>`).join('')}
            </div>
        `;
    }
}

function compareBreeds() {
    const breed1 = document.getElementById('breed1').value;
    const breed2 = document.getElementById('breed2').value;
    const resultDiv = document.getElementById('comparison-result');
    
    if (breed1 && breed2 && breed1 !== breed2) {
        resultDiv.innerHTML = `
            <div class="comparison-content">
                <p>Comparing ${breed1.replace('-', ' ')} vs ${breed2.replace('-', ' ')}</p>
                <a href="#compare-${breed1}-${breed2}">View Detailed Comparison ‚Üí</a>
            </div>
        `;
    }
}
</script>